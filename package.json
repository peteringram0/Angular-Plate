{
  "name": "angularjs-plate",
  "version": "0.3.0",
  "private": true,
  "authors": [
    {
      "name": "Peter Ingram",
      "email": "peter.ingram0@gmail.com"
    }
  ],
  "dependencies": {
    "angular": "^1.5.7",
    "angular-ui-router": "^0.2.18",
    "normalize.css": "^3.0.3"
  },
  "settings": {
    "vendor": [
      "node_modules/angular/angular.min.js",
      "node_modules/angular-ui-router/release/angular-ui-router.min.js"
    ],
    "partials": [
      "build/**/*.tpl.html",
      "!build/index.html"
    ],
    "scripts": [
      "build/**/*.js",
      "!build/assets/*.js"
    ],
    "scriptsOrder": [
      "app.js",
      "**/*.js"
    ],
    "styles": "build/styles/styles.scss",
    "assets": "build/assets/**/*.*",
    "index": "build/index.html"
  },
  "devDependencies": {
    "browser-sync": "latest",
    "connect-history-api-fallback": "^1.2.0",
    "del": "latest",
    "git-rev": "latest",
    "gulp": "latest",
    "gulp-angular-filesort": "^1.1.1",
    "gulp-chmod": "latest",
    "gulp-clean": "latest",
    "gulp-concat": "latest",
    "gulp-concat-css": "latest",
    "gulp-htmlmin": "^2.0.0",
    "gulp-if": "latest",
    "gulp-load-plugins": "^1.2.4",
    "gulp-minify-css": "latest",
    "gulp-ng-html2js": "latest",
    "gulp-order": "latest",
    "gulp-plumber": "latest",
    "gulp-preprocess": "latest",
    "gulp-rename": "latest",
    "gulp-scss": "latest",
    "gulp-sourcemaps": "latest",
    "gulp-uglify": "latest",
    "gulp-watch": "latest",
    "http-server": "latest",
    "jasmine-core": "latest",
    "jshint-stylish": "latest",
    "requirejs": "latest",
    "shelljs": "latest",
    "streamqueue": "^1.1.1"
  }
}
